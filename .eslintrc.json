{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
      "no-useless-catch": 0,
      "comma-dangle": 0,
      "import/prefer-default-export": 0,
      "linebreak-style": 0
  }
}